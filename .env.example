# Copy this file to .env and fill in your values if you want to use environment variables.
# For most users, the CLI configuration flow is recommended.

# Gemini API Key (required if not using the CLI configure flow)
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Override default safety level (high, medium, low)
TERMIGENIUS_SAFETY_LEVEL=high

# Optional: Enable verbose logging
TERMIGENIUS_VERBOSE=false